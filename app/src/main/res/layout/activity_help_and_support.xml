<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.roushan.mnitflightbookiing.HelpAndSupport"
    android:weightSum="1">
    <View
        android:id="@+id/f"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="63dp"
        android:background="@android:color/darker_gray"/>
    <View
        android:id="@+id/s"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="129dp"
        android:background="@android:color/darker_gray"/>
    <View
        android:id="@+id/t"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="195dp"
        android:background="@android:color/darker_gray"/>




    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:onClick="progress"
        app:srcCompat="@drawable/question" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView8"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_toRightOf="@+id/imageView8"
        android:text="Frequently Asked Question"
        android:onClick="progress"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView30"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:text=">"
        android:onClick="progress"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        app:srcCompat="@drawable/feedback"
        android:layout_below="@+id/f"
        android:layout_alignLeft="@+id/imageView8"
        android:onClick="progress"
        android:layout_alignStart="@+id/imageView8" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Feedback"
        android:textSize="18sp"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_alignLeft="@+id/textView30"
        android:onClick="progress"
        android:layout_alignStart="@+id/textView30"
        android:layout_alignRight="@+id/textView30"
        android:layout_alignEnd="@+id/textView30" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=">"
        android:textSize="24sp"
        android:layout_alignBottom="@+id/textView32"
        android:layout_alignLeft="@+id/textView31"
        android:onClick="progress"
        android:layout_alignStart="@+id/textView31" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        app:srcCompat="@drawable/favorite"
        android:layout_below="@+id/imageView9"
        android:layout_alignLeft="@+id/imageView9"
        android:onClick="progress"
        android:layout_alignStart="@+id/imageView9" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rate this app"
        android:textSize="18sp"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_alignLeft="@+id/textView32"
        android:onClick="progress"
        android:layout_alignStart="@+id/textView32"
        android:layout_alignRight="@+id/textView32"
        android:layout_alignEnd="@+id/textView32" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=">"
        android:textSize="24sp"
        android:layout_alignBottom="@+id/textView34"
        android:layout_alignLeft="@+id/textView33"
        android:onClick="progress"
        android:layout_alignStart="@+id/textView33" />
</RelativeLayout>
